{
  "name": "SmartPay",
  "version": "1.0",
  "manifest_version": 2,
  "background": [
    {
      "scripts": ["thirdParty/jquery-2.0.3.js", "background.js"],
      "persistent": false
    }
  ],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://shopee.sg/*"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SmartPay"
  }

}